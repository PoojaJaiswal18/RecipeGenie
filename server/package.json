    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "esm": "^3.2.25",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
